<div class="container mt-4">
  <h2>User TACO Balance</h2>
  <table class="table">
    <thead>
      <tr class="table-active">
        <th class="text-uppercase text-start">Guild</th>
        <th class="text-uppercase text-start">User</th>
        <th class="text-uppercase text-end">Count</th>
        <th class="text-uppercase"></th>
      </tr>
    </thead>
    <tbody>
      {{#each items}}
      <tr>
        <td class="text-nowrap p-1 text-start">{{> discord/guild id=this.guild_id}}</td>
        <td class="text-nowrap p-1 text-start">{{> discord/user user_id=this.user_id guild_id=this.guild_id}}</td>
        <td class="text-nowrap p-1 text-end me-1">{{> taco count=this.count}}</td>
        <td class="text-nowrap"></td>
      {{/each}}
    </tbody>
  </table>
  {{> pager this.pager}}
</div>
